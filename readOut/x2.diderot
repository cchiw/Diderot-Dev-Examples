
int outSize = 10;


field#4(3)[] V = bspln5 ⊛ image("../data/parab4.nrrd");
field#3(3)[3] D = ∇V/|∇V|;
field#3(3)[] M = ∇V•∇V;
field#2(3)[] F = -∇M • D;


strand sample (int ui, int vi) {
    vec3 x = [6,11,23];
    // output real val = F(x);
    output vec3 grad = -∇F(x);

    update {
             stabilize;
    }
}

initially [ sample(ui, vi) | vi in 0..(outSize-1), ui in 0..(outSize-1) ];
