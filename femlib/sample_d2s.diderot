input real stepSize;
input int imgResU;
input int imgResV;
input fem#0(2)[] f;
string data = "/Users/chariseechiw/diderot/femprime/jsonstuff/example.json";
ofield#0(2)[] F = convert(f, data);

strand RayCast (int ui, int vi) {
    vec2 pos=[ui,vi]*stepSize;    
    output real out=0.0;
   
    update {
        out= inst(F,pos);

        stabilize;
    }
}
initially [ RayCast(ui, vi) | vi in 0..imgResV-1, ui in 0..imgResU-1];